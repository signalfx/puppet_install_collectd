{
  "name": "signalfx-install_collectd",
  "version": "0.1.2",
  "author": "Uday Sagar Shiramshetty",
  "summary": "A puppet module that just installs collectd from SignalFx repositories",
  "license": "Apache-2.0",
  "source": "https://github.com/signalfx/puppet_install_collectd",
  "project_page": "https://github.com/signalfx/puppet_install_collectd",
  "issues_url": "https://github.com/signalfx/puppet_install_collectd/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2015.03",
        "2014.09"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "15.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0"
    }
  ],
  "dependencies": [
    {"name":"pdxcat/collectd","version_requirement":">= 4.0.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">= 3.2.0"},
    {"name":"puppetlabs/concat","version_requirement":">= 1.0.4"}
  ]
}
